{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/components/content/*"],
      "@navigation/*": ["./src/components/navigation/*"],
      "@theme/*": ["./src/components/theme/*"],
      "@image/*": ["./src/components/image/*"],
      "@image": ["./src/components/image/Image.tsx"],
      "@utils/*": ["./src/utils/*"],
      "@time": ["./src/time.ts"],
      "@useTheme": ["./src/utils/useTheme.ts"],
      "@package": ["./package.json"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../v3.royalfoxy.xyz/src/components/Content/Text.tsx",
    "../v3.royalfoxy.xyz/src/components/Content/Title.tsx",
    "src/utils/userAgent"
  ],
  "exclude": ["node_modules"]
}
